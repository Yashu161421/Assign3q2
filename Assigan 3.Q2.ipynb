   ],
   "source": [
    "def reverse(number):\n",
    "    string = str(number)\n",
    "    reverse = string[::-1]\n",
    "    return \"\".join(reverse)\n",
    "\n",
    "number = eval(input(\"Enter a number :\"))\n",
    "reverse(number)"
   ]
